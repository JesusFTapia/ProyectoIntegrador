            layoutX="256.0" layoutY="88.0"
            onAction="#cargarVistaUno"
            style="-fx-background-color: #9C7277;
                                           -fx-text-fill: white;
                                           -fx-background-radius: 8;
                                           -fx-padding: 12 24;
